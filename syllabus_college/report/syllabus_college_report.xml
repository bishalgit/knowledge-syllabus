<odoo>

    <report
        id="report_syllabus_college"
        model="syllabus_college.package"
        string="Syllabus Package Report"
        name="syllabus_college.report_syllabus_package_view"
        file="syllabus_college.report_syllabus_package"
        report_type="qweb-pdf" />

    <template id="report_syllabus_package_view">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-foreach="doc.syllabus_ids" t-as="syllabus">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <span t-field="syllabus.content"></span>
                        </div>
                    </t>
                </t>
            </t>
        </t>
    </template>

</odoo>